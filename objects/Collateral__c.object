    <validationRules>
        <fullName>Asset_Description</fullName>
        <active>true</active>
        <description>If a Collateral Item has any underlying assets the user must put in a description of that/those assets</description>
        <errorConditionFormula>AND(
    ISPICKVAL(Has_Underlying_Assets__c, &quot;True&quot;),
  ISBLANK( Asset_Description__c )
)</errorConditionFormula>
        <errorDisplayField>Asset_Description__c</errorDisplayField>
        <errorMessage>Please ensure that you specify the description of the Asset when asset is set to &quot;True&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Supporting_Documentation_Active_Status</fullName>
        <active>true</active>
        <description>Prompts agent to confirm whether support documentation has been uploaded when the status is changed to Active.</description>
        <errorConditionFormula>IF(AND(TEXT(Status__c) = &quot;Active&quot; ,AND((ISBLANK(Comments__c )),  Supporting_Documentation_Uploaded__c =True)),True, False)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Please enter the remark detail for the collateral item</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Documents_Are_Verified</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   OR
   (
      ISPICKVAL(Status__c, &quot;Active&quot;),
      ISPICKVAL(Status__c , &quot;Released&quot;) 
   ), 
   NOT(Supporting_Documentation_Uploaded__c)
 )</errorConditionFormula>
        <errorDisplayField>Supporting_Documentation_Uploaded__c</errorDisplayField>
        <errorMessage>Please upload the relevant documentation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Value_Field_validation</fullName>
        <active>true</active>
        <description>Validates that a custom field called  Value contains a value if a Value type has a type of “Limited.”</description>
        <errorConditionFormula>AND(
  ISPICKVAL( Value_Type__c , &quot;Limited&quot;) ,
 ISNULL( Value__c )
)</errorConditionFormula>
        <errorDisplayField>Value__c</errorDisplayField>
        <errorMessage>Ensure that the Value is specified when the type is set to Limited.</errorMessage>
    </validationRules>

    <validationRules>
        <fullName>DNQ_Lead_requires_Referred_to</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.Id = &quot;0121p000000kRAJ&quot; &amp;&amp;
ISPICKVAL ( Client_Type__c ,&quot;DNQ&quot;) &amp;&amp;
ISPICKVAL ( Status,&quot;Unqualified&quot;) &amp;&amp;
 ISBLANK (Referred_to__c)</errorConditionFormula>
        <errorDisplayField>Referred_to__c</errorDisplayField>
        <errorMessage>Referred to and Referred to Business Unit  are required for status DNQ</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Referred_to_requires_Referred_to_BU</fullName>
        <active>true</active>
        <description>Wealth Leads</description>
        <errorConditionFormula>RecordType.Id = &quot;0121p000000kRAJ&quot; &amp;&amp;
NOT ISBLANK (Referred_to__c )&amp;&amp;
((NOT ((ISPICKVAL (Referred_to_BU__c, &quot;ABSA Private Bank&quot;)||
 ISPICKVAL (Referred_to_BU__c, &quot;AIFA&quot;)||
 ISPICKVAL (Referred_to_BU__c, &quot;CIB&quot;)||
ISPICKVAL (Referred_to_BU__c, &quot;RBB&quot;)||
 ISPICKVAL (Referred_to_BU__c, &quot;Other&quot;)))))</errorConditionFormula>
        <errorDisplayField>Referred_to_BU__c</errorDisplayField>
        <errorMessage>Please complete</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Qualified_requires_Needs</fullName>
        <active>true</active>
        <description>If Lead status is updated to Qualified, the Standing and Needs fields need to be populated</description>
        <errorConditionFormula>AND(ISPICKVAL(Status, &quot;Qualified&quot;)&amp;&amp; 
 
ISBLANK (TEXT(NAV__c) ))</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Please complete Standing and Needs section below in order to Qualify lead</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unqualified_Lead_requires_Review_Date</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.Id = &quot;0121p000000kRAJ&quot;&amp;&amp;
(ISPICKVAL ( Status,&quot;Contacted&quot;) ||
ISPICKVAL ( Status,&quot;Unqualified&quot;))&amp;&amp;
ISBLANK (Review_Date__c)</errorConditionFormula>
        <errorDisplayField>Review_Date__c</errorDisplayField>
        <errorMessage>Please insert a date for future review</errorMessage>
    </validationRules>

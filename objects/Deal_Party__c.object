    <validationRules>
        <fullName>ECA_Part_Type_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Party_Type__c , &quot;ECA&quot;),
 Deal__r.ECA_backed__c = FALSE)</errorConditionFormula>
        <errorMessage>Please ensure that ECA Party Type can only be selected if the ECA Backed is checked on the Deal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LIBRA_No_CounterP_or_CISC_Added</fullName>
        <active>true</active>
        <description>Limits CNSG Team from adding a Counterparty and Collective Investment Scheme Manager as a Deal Party excluding the auto-created Deal Parties from Matter creation</description>
        <errorConditionFormula>IF(NOT(ISCHANGED(Relationship__c)), AND((Deal__r.Count_Collective_Investment_Scheme_Man__c = 1 ||  Deal__r.Count_Counterparty__c = 1 ||  Deal__r.Count_Asset_Manager__c = 1), ISPICKVAL(Relationship_Type__c, &quot;Counterparty&quot;) || ISPICKVAL(Relationship_Type__c, &quot;Collective Investment Scheme Manager&quot;)), false)</errorConditionFormula>
        <errorMessage>You are not allowed to add an additional Counterparty or Collective Investment Scheme Manager</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Libra_Counterparty_Branch_Validation</fullName>
        <active>true</active>
        <description>Validation to limit users from linking counterparty branches when the market transaction relationship type = &quot;collective scheme investment manager&quot; or &quot;asset manager&quot;</description>
        <errorConditionFormula>AND(ISPICKVAL(Relationship_Type__c, &quot;Counterparty Branch&quot;) , ISPICKVAL(Deal__r.Relationship_Type_picklist__c, &quot;Asset Manager&quot;) || ISPICKVAL(Deal__r.Relationship_Type_picklist__c,&quot;Collective Investment Scheme Manager&quot;))</errorConditionFormula>
        <errorMessage>A Deal Party of this type cannot be added to an Agreement created for an Asset Manager or a Collective Investment Scheme Manager</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Libra_Counterparty_Deal_P</fullName>
        <active>true</active>
        <description>Validation to limit users from a adding a disclosed principal deal party when the matter relationship is of counterparty relationship type</description>
        <errorConditionFormula>AND(Counterparty__c = TRUE, ISPICKVAL(Relationship_Type__c ,&quot;Disclosed Principal&quot;))</errorConditionFormula>
        <errorMessage>You cannot add a Disclosed Principal as a deal party to a Counterparty relationship.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Libra_Hedge_Funds_Validation_for_MT</fullName>
        <active>true</active>
        <description>Validation to limit users from linking retail hedge fund and qualified investor hedge funds when the market transaction relationship type = &quot;counterparty&quot; or &quot;asset manager&quot;.</description>
        <errorConditionFormula>AND(ISPICKVAL(Relationship_Type__c, &quot;Retail Hedge Fund&quot;) || ISPICKVAL(Relationship_Type__c, &quot;Qualified Investor Hedge Fund&quot;), ISPICKVAL(Deal__r.Relationship_Type_picklist__c, &quot;Asset Manager&quot;) || ISPICKVAL(Deal__r.Relationship_Type_picklist__c,&quot;Counterparty&quot;))</errorConditionFormula>
        <errorMessage>You cannot add Asset Manager or Counterparty relationship to this type of Deal Party.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Libra_Only_One_Fund_Manager_Allowed</fullName>
        <active>true</active>
        <errorConditionFormula>Fund_Managers__c &gt; 1</errorConditionFormula>
        <errorMessage>You are only allowed to link one Fund Manager to the Deal Party</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Libra_Validate_Hedge_Fund_Parties</fullName>
        <active>true</active>
        <description>Only allowed to add Hedge Fund Parties to Deal Parties of type &quot; Qualified Investor Hedge Fund&quot; and &quot;Retail Hedge Fund&quot;</description>
        <errorConditionFormula>IF(AND(ISPICKVAL(Relationship_Type__c , &apos;Qualified Investor Hedge Fund&apos;) == FALSE , ISPICKVAL(Relationship_Type__c , &apos;Retail Hedge Fund&apos;)== FALSE ,OR( Fund_Managers__c &gt; 0,  Investment_Managers__c  &gt; 0)), TRUE, FALSE)</errorConditionFormula>
        <errorMessage>You are not allowed to link a Hedge Fund Party to this Deal Party, please contact your Salesforce Administrator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TMG_Must_be_a_TA_or_TM_role</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    NOT(ISBLANK(Conditions_Monitored_By__c)),
   Deal__r.RecordType.DeveloperName = &quot;Deal&quot;,
   NOT(  
      INCLUDES(Conditions_Monitored_By__r.advpm__Role__c, &quot;Transaction Administration&quot;)
      ),
    NOT(  
      INCLUDES(Conditions_Monitored_By__r.advpm__Role__c, &quot;Transaction Management&quot;)
      ),
 NOT(  
      INCLUDES(Conditions_Monitored_By__r.advpm__Role__c, &quot;Agency&quot;)
      ),
    NOT(  
      INCLUDES(Conditions_Monitored_By__r.advpm__Role__c, &quot;SPV&quot;)
      )
)</errorConditionFormula>
        <errorDisplayField>Conditions_Monitored_By__c</errorDisplayField>
        <errorMessage>You must select a participant with the role of &quot;Transaction Management&quot; or &quot;Transaction Administration&quot; or &quot;SPV&quot; or &quot;Agency&quot; to monitor conditions for a borrower.</errorMessage>
    </validationRules>

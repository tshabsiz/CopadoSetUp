		ISPICKVAL(ProductLevel3__c,&apos;Domestic Collections (Online - Direct Debits)&apos;),
		ISPICKVAL(ProductLevel3__c,&apos;Domestic Collections (Online - NAEDO)&apos;),
		ISPICKVAL(ProductLevel3__c,&apos;Domestic Payments (Online - Payments)&apos;),
		ISPICKVAL(ProductLevel3__c,&apos;Information Product (Online - Statement Delivery)&apos;)

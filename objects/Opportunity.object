        <fullName>Reason_for_Won</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
$Profile.Name &lt;&gt; &apos;Barclays Administrator&apos;, 
RecordTypeId &lt;&gt; &apos;01224000000TOY1&apos;, 
RecordTypeId &lt;&gt; &apos;01224000000TOY0&apos;, 
ISPICKVAL(StageName, &quot;Closed Won&quot;), 

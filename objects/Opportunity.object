    <validationRules>
        <fullName>Cannot_modify_Close_Date_field</fullName>
        <active>true</active>
        <description>Close Date field can not be modified after Opportunity was previously Won or Lost</description>
        <errorConditionFormula>AND (
 $Profile.Name &lt;&gt; &apos;Barclays Administrator&apos;,
 NOT(ISCHANGED(StageName)), (TEXT( StageName ) = &quot;Closed Won&quot; || TEXT (StageName) = &quot;Closed Lost&quot;),
( ISCHANGED( CloseDate ))
)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Close Date cannot be amended after the Opportunity Status is Won / Lost. 
Please contact SF Support for assistance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_date_change</fullName>
        <active>true</active>
        <description>Requires reason for change if close date field has been modified</description>
        <errorConditionFormula>AND(
$User.ProfileId = &apos;00e24000001AULo&apos;,
ISCHANGED( CloseDate ),
Reason_for_close_date_change__c = &quot;&quot;)</errorConditionFormula>
        <errorMessage>Please enter a reason for close date change</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closing_Opportunity_Pending_Approval</fullName>
        <active>true</active>
        <description>Stops a user from closing an opportunity while the Opportunity is still pending approval from the Product Partner</description>
        <errorConditionFormula>$User.ProfileId &lt;&gt; &quot;00e24000000mKlR&quot; 
&amp;&amp; 
$User.ProfileId &lt;&gt; &quot;00e24000000obeD&quot;
&amp;&amp;
(  
(NOT(ISNEW()) &amp;&amp; ISCHANGED(StageName) &amp;&amp; ISPICKVAL(StageName, &quot;Closed Won&quot;))
)
&amp;&amp;
ISPICKVAL(ProductPartnerApprovalStatus__c, &quot;Pending&quot;)</errorConditionFormula>
        <errorMessage>This opportunity is pending a Product Partner acknowledgement and can not be Closed Won</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_a_referral_name_on_a_referred_Opp</fullName>
        <active>true</active>
        <description>Request the user to enter a User as a referrer if opportunity is referred</description>
        <errorConditionFormula>And (
ISPICKVAL(CIB_Referral__c, &apos;Yes&apos;), 
 Name_of_CIB_Referrer__c =&quot;&quot;)</errorConditionFormula>
        <errorMessage>Please enter a referrer name for referred opportunities</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>First_Drawdown_Mandatory</fullName>
        <active>true</active>
        <description>First drawdown date and first drawdown amount must be mandatory for non-Oberon linked debt family opportunities for stage initial solution proposed and later (CIBDIGSALP-2294)</description>
        <errorConditionFormula>AND 
( 
$Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
$Profile.Name &lt;&gt; &apos;Barclays Administrator&apos;,
OR(ISBLANK(First_Drawdown_Manual__c),
   ISBLANK( First_Drawdown_Amount__c) 
),
OR(
ISCHANGED(StageName),
ISCHANGED(First_Drawdown_Manual__c),
ISCHANGED(First_Drawdown_Amount__c)
),
OR(
   ISPICKVAL(StageName, &apos;Initial Solution Proposed&apos;),
   ISPICKVAL(StageName, &apos;Credit Case Submitted&apos;),
   ISPICKVAL(StageName, &apos;Credit Sanctions Obtained&apos;),
   ISPICKVAL(StageName, &apos;Pricing Finalised&apos;),
   ISPICKVAL(StageName, &apos;Mandate Won&apos;),
   ISPICKVAL(StageName, &apos;Contracts Finalised&apos;),
   ISPICKVAL(StageName, &apos;In Implementation / Conditions Precedent&apos;),
   ISPICKVAL(StageName, &apos;Closed Won&apos;)
),
NOT(isExternal__c),
NOT(CONTAINS(ProductLevel2List__c, &apos;Resource and Project Finance&apos;)),
CONTAINS(ProductList__c, &apos;Global Finance&apos;),
OR(
   RecordTypeId = &apos;01224000000Kx2m&apos;,
   RecordTypeId = &apos;01224000000Kx2n&apos;
)
)</errorConditionFormula>
        <errorDisplayField>First_Drawdown_Manual__c</errorDisplayField>
        <errorMessage>First Drawdown Date and Amount is Mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Financial_information_2_60</fullName>
        <active>true</active>
        <description>Makes Financial information mandatory when probability is &gt;60% (Investment)</description>
        <errorConditionFormula>AND( 
RecordTypeId = &apos;01224000000TOY1&apos;,  
Probability &gt;= 0.6, 
ISBLANK( Amount ) || 
ISBLANK( Margin__c ) || 
ISBLANK( Percentage__c ))</errorConditionFormula>
        <errorMessage>You must enter information in the Income Summary fields when the Probability of the Opportunity is greater than 60%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Financial_information_60</fullName>
        <active>true</active>
        <description>Makes Financial information mandatory when probability is &gt;60% (Credit and Banking)</description>
        <errorConditionFormula>AND(

    <fields>
        <fullName>AccountId</fullName>
        <inlineHelpText>Only Relationship Groups or Prospect Relationships can be related to an Opportunity</inlineHelpText>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>((1 OR 5) AND 2) OR ((3 OR 5) AND 4) OR ((5 OR 6) AND 7)</booleanFilter>
            <errorMessage>Opportunities can only be added to Relationship Group, Prospects, Prospects - Individual and Prospect - Legal Entity record types of the Relationship.</errorMessage>
            <filterItems>
                <field>$Profile.Name</field>
                <operation>equals</operation>
                <value>Barclays Standard User, ABSA RAISS User, Standard User ( Lightning ), Standard User (WIMI), Standard User (Banking - Private), Standard User (Markets), Standard User ( Private Lightning ), Standard User (Research), Standard User (TBS User), Standard User</value>
            </filterItems>
            <filterItems>
                <field>Account.RecordTypeId</field>
                <operation>equals</operation>
                <value>Relationship Group, Prospect</value>
            </filterItems>
            <filterItems>
                <field>$Profile.Name</field>
                <operation>equals</operation>
                <value>Barclays Wealth Advisor, Standard User (Wealth Advisory)</value>
            </filterItems>
            <filterItems>
                <field>Account.RecordType.DeveloperName</field>
                <operation>notEqual</operation>
                <value>Client - Individual, Client - Legal Entity, Prospect - Individual, Prospect - Legal Entity</value>
            </filterItems>
            <filterItems>
                <field>$Profile.Name</field>
                <operation>equals</operation>
                <value>Barclays Private Side User,Barclays Administrator, Delegated Admins, System Administrator, Barclays Business Owner, Barclays API User, Standard User (RoA)</value>
            </filterItems>
            <filterItems>
                <field>$Profile.Name</field>
                <operation>equals</operation>
                <value>Standard User (WIMI), Standard User (Retail Distribution)</value>
            </filterItems>
            <filterItems>
                <field>Account.RecordType.DeveloperName</field>
                <operation>contains</operation>
                <value>EDC</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>true</trackHistory>
        <type>Lookup</type>
    </fields>

    <validationRules>
        <fullName>Bullet_Amount</fullName>
        <active>true</active>
        <description>Bullet Amount can not be greater than hold size</description>
        <errorConditionFormula>OR(
	AND(
		NOT(ISBLANK(BulletAmount__c)),
		NOT(ISBLANK(Hold_Size__c)),
		BulletAmount__c &gt; Hold_Size__c
	)
	,
	AND
	(
		NOT(ISBLANK(BulletAmount__c)),
		BulletAmount__c &lt; 0
	)
)</errorConditionFormula>
        <errorMessage>Bullet Amount can not be greater than Hold Size or less than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FTP</fullName>
        <active>true</active>
        <description>Makes sure FTP is between -10 and 99.99</description>
        <errorConditionFormula>OR 
( 
Ftp__c &lt; -10, 
Ftp__c &gt; 99.99
)</errorConditionFormula>
        <errorMessage>FTP must be between -10 and 99.99</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fees</fullName>
        <active>false</active>
        <description>The Annualized Income formula will only work if there is atleast a 0 value in the fees field</description>
        <errorConditionFormula>ISBLANK(Fees__c)</errorConditionFormula>
        <errorDisplayField>Fees__c</errorDisplayField>
        <errorMessage>There must at least be a 0 in the Fees field in order to save a product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Negative_Fees</fullName>
        <active>true</active>
        <description>Checks if any fees entered are negative</description>
        <errorConditionFormula>OR(
	AND
	(
		NOT(ISBLANK(Arranging_Fee__c)),
		Arranging_Fee__c &lt; 0
	),
	AND
	(
		NOT(ISBLANK(Other_Fees__c)),
		Other_Fees__c &lt; 0
	),
	AND
	(
		NOT(ISBLANK(Committment_Fee__c)),
		Committment_Fee__c &lt; 0
	),
	AND
	(
		NOT(ISBLANK(FlexiFee__c)),
		FlexiFee__c &lt; 0
	),
	AND
	(
		NOT(ISBLANK(Annual_Facility_Fee__c)),
		Annual_Facility_Fee__c &lt; 0
	),
	AND
	(
		NOT(ISBLANK(Participation_Fee__c)),
		Participation_Fee__c &lt; 0
	),
	AND
	(
		NOT(ISBLANK(Agency_Fee__c)),
		Agency_Fee__c &lt; 0
	),
	AND
	(
		NOT(ISBLANK(Co_ordination_Fee__c)),
		Co_ordination_Fee__c &lt; 0
	)
)</errorConditionFormula>
        <errorMessage>Fees can not be less than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Refinance</fullName>
        <active>true</active>
        <description>Refinance Amount can not be greater than hold size</description>
        <errorConditionFormula>OR(
	AND(
		NOT(ISBLANK(RefinanceAmount__c)),
		NOT(ISBLANK(Hold_Size__c)),
		RefinanceAmount__c &gt; Hold_Size__c
	)
	,
	AND
	(
		NOT(ISBLANK(RefinanceAmount__c)),
		RefinanceAmount__c &lt; 0
	)
)</errorConditionFormula>
        <errorMessage>Refinance Amount can not be greater than Hold Size or less than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Term_Years</fullName>
        <active>true</active>
        <description>Makes sure that the term years don&apos;t go over 99.99999 or under 0</description>
        <errorConditionFormula>OR( Terms_Years__c &lt; 0, Terms_Years__c &gt; 99.99999)</errorConditionFormula>
        <errorMessage>The term must be between 0 and 99 years</errorMessage>
    </validationRules>

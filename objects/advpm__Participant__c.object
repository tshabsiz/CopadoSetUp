    <validationRules>
        <fullName>Assign_Signing_Authority</fullName>
        <active>true</active>
        <errorConditionFormula>AND($UserRole.Name    &lt;&gt;   &quot;TMG - TA Deal Management - Governance and Doc Solution&quot;, $UserRole.Name    &lt;&gt;   &quot;TMG - TA Deal Management - Governance and Doc Solution TM&quot;,  Signing_Authority__c = TRUE)</errorConditionFormula>
        <errorMessage>Kindly note that you cannot assign Signing Authority if your Role is not TMG - TA Deal Management - Governance and Doc Solution.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Number_Validation</fullName>
        <active>true</active>
        <description>Participant Contact Number</description>
        <errorConditionFormula>NOT(ISBLANK( Contact_Number__c )) &amp;&amp; 
NOT(REGEX( Contact_Number__c , &quot;[0-9]{4,5}[&apos; &apos;][0-9]{3}[&apos; &apos;][0-9]{4,7}&quot;))</errorConditionFormula>
        <errorDisplayField>Contact_Number__c</errorDisplayField>
        <errorMessage>The correct format is - Country Code, Area Code Space Three Digits Space Remainder Digits. eg. 2711 237 1434</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Business_Fields_Are_Completed</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
  INCLUDES(advpm__Role__c,&quot;Matter Owner&quot;),
  OR(
     ISBLANK(TEXT(Business_Unit_Litigation__c)),
     ISBLANK(TEXT(Cluster_Litigation__c )),
     ISBLANK(TEXT(Segment_Litigation__c ))
), RecordType.DeveloperName = &quot;User&quot;
)</errorConditionFormula>
        <errorMessage>Please ensure you specify a &quot;Cluster&quot;, &quot;Segment&quot; and a &quot;Business Unit&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Business_Fields_Are_Completed2</fullName>
        <active>false</active>
        <errorConditionFormula>ISBLANK(TEXT(Cluster_Litigation__c ))</errorConditionFormula>
        <errorMessage>Please ensure you specify a &quot;Cluster&quot;, &quot;Segment&quot; and a &quot;Business Unit&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Legal_User_on_Legal_Matter_Check</fullName>
        <active>true</active>
        <description>Only Legal Users (where Profile contains Legal) can be added to a Legal Matter.</description>
        <errorConditionFormula>IF(OR(CONTAINS(advpm__Matter__r.RecordType.DeveloperName, &apos;Legal&apos;), CONTAINS(advpm__Matter__r.RecordType.DeveloperName, &apos;Litigation&apos;)) &amp;&amp; !CONTAINS(advpm__Staff__r.Profile.Name, &apos;Litigation&apos;) &amp;&amp; !CONTAINS(advpm__Staff__r.Profile.Name, &apos;Legal&apos;) &amp;&amp; !CONTAINS(advpm__Staff__r.Profile.Name, &apos;Content&apos;) &amp;&amp; !CONTAINS(advpm__Staff__r.Profile.Name, &apos;Support&apos;), TRUE, FALSE)</errorConditionFormula>
        <errorMessage>You only allowed to add a Legal/Litigation User as a Participant for a Legal/Litigation Matter</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Litigation_User_on_Matter_Check</fullName>
        <active>false</active>
        <description>Only Litigation Users  (where Profile contains Litigation) can be added to a Litigation Matter</description>
        <errorConditionFormula>IF(AND(CONTAINS(advpm__Matter__r.RecordType.DeveloperName, &apos;Litigation&apos;), !CONTAINS(advpm__Staff__r.Profile.Name, &apos;Litigation&apos;)), True, False)</errorConditionFormula>
        <errorMessage>You only allowed to add a Litigation User as a Participant for a Litigation Matter</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Primary_Contact_must_be_specified_TA</fullName>
        <active>false</active>
        <description>When a role is allocated to more than one participant, primary contact must be specified - Transaction Administrator</description>
        <errorConditionFormula>IF(
INCLUDES( advpm__Role__c , &quot;Transaction Administration&quot;), True,
      Primary_Contact_for_Role__c = FALSE)</errorConditionFormula>
        <errorDisplayField>Primary_Contact_for_Role__c</errorDisplayField>
        <errorMessage>Please specify  if you are the Primary Contact for Role</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Primary_Contact_must_be_specified_TM</fullName>
        <active>false</active>
        <description>When a role is allocated to more than one participant, primary contact must be specified - Transaction Manager</description>
        <errorConditionFormula>IF(
INCLUDES( advpm__Role__c , &quot;Transaction Management&quot;), True,
      Primary_Contact_for_Role__c = TRUE)</errorConditionFormula>
        <errorDisplayField>Primary_Contact_for_Role__c</errorDisplayField>
        <errorMessage>Please specify  if you are the Primary Contact for Role</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_One_Role_For_Each_Participant</fullName>
        <active>true</active>
        <errorConditionFormula>IF(INCLUDES(advpm__Role__c, &quot;Agency&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;CCMG&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;CIB Operations&quot;),1,0) +
IF(INCLUDES(advpm__Role__c, &quot;Client Service Consultant&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;RF Operations&quot;),1,0) +
IF(INCLUDES(advpm__Role__c, &quot;STCF Operations&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Client&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Credit Analyst&quot;),1,0)+
IF(INCLUDES(advpm__Role__c, &quot;DCM&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Finance&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Legal Counsel&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Portfolio Management Private Side&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Product&quot;),1,0)+
IF(INCLUDES(advpm__Role__c, &quot;Product Control Group&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Regulatory Capital&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Relationship Executive&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Relationship Executive Support&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Risk&quot;),1,0)+
IF(INCLUDES(advpm__Role__c, &quot;SPV&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;TAX&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Technical Accounting&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Transaction Administration&quot;),1,0)+
IF(INCLUDES(advpm__Role__c, &quot;Banker&quot;),1,0)+
IF(INCLUDES(advpm__Role__c, &quot;Corporate Service Manager&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Credit Head&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Sector Head&quot;),1,0) + 
IF(INCLUDES(advpm__Role__c, &quot;Regional Head&quot;),1,0)+
IF(INCLUDES(advpm__Role__c, &quot;Transaction Management&quot;),1,0) &gt;1</errorConditionFormula>
        <errorDisplayField>advpm__Role__c</errorDisplayField>
        <errorMessage>Please ensure that only one Role is selected for each Participant</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TMG_Participant_Role</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK( advpm__Role__c ),
     (RecordType.DeveloperName  = &quot;Staff&quot;)
   )</errorConditionFormula>
        <errorDisplayField>advpm__Role__c</errorDisplayField>
        <errorMessage>Please ensure that you have specified a Role.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>advpm__Staff_Account_And_Contact_Verify</fullName>
        <active>true</active>
        <errorConditionFormula>OR
(
  AND
  (
   NOT( ISBLANK( advpm__Staff__c ) ), NOT( ISBLANK( advpm__Contact__c ) )
  ),
  AND
  (
   NOT( ISBLANK( advpm__Staff__c ) ), NOT( ISBLANK( advpm__Account__c ) ) 
  ),
  AND
  (
   NOT( ISBLANK( advpm__Contact__c ) ), NOT( ISBLANK( advpm__Account__c ) ) 
  )
)</errorConditionFormula>
        <errorMessage>Combination of &apos;User&apos;, &apos;Contact&apos; and &apos;Account&apos; cannot be saved as  Participant. Only 1 of them can be a participant.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>advpm__Staff_OR_Account_OR_Contact_Is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK( advpm__Staff__c ), ISBLANK( advpm__Contact__c ), ISBLANK( advpm__Account__c ))</errorConditionFormula>
        <errorMessage>Either &apos;User&apos;, &apos;Contact&apos; or &apos;Account&apos; must be provided.</errorMessage>
    </validationRules>

    <webLinks>
        <fullName>Cancel_Drawdown</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel Drawdown</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/apex.js&quot;)} 

var confirmation = confirm(&quot;Are you sure you want to cancel this drawdown?&quot;);

if(confirmation == true)
{
	var soqlQuery = sforce.connection.query(&quot;SELECT Id, Status__c FROM Drawdown__c WHERE Id =&apos;{!Drawdown__c.Id}&apos; LIMIT 1&quot;);
	var records = soqlQuery.getArray(&quot;records&quot;); 
	var currentStatus = records[0].Status__c;

	if(currentStatus != &apos;Cancelled&apos;)
	{
		//Identify the record.
		var drawdownRecord = new sforce.SObject(&quot;Drawdown__c&quot;);
		drawdownRecord.Id = &apos;{!Drawdown__c.Id}&apos;;

		drawdownRecord.Status__c = &quot;Cancelled&quot;;

		//Save the change
		sforce.connection.update([drawdownRecord]);	

		//refresh the page
		window.location.reload();
	}
	
	if(currentStatus == &apos;Cancelled&apos;)
	{
		alert(&apos;This drawdown has already been cancelled.&apos;)
	}
}</url>
    </webLinks>

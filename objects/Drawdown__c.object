    <validationRules>
        <fullName>Draw_Down_Completed</fullName>
        <active>true</active>
        <description>Please ensure &quot;Supporting Documentation Uploaded&quot; is checked for a completed drawdown.</description>
        <errorConditionFormula>AND(
   ISCHANGED(Status__c),
   ISPICKVAL(Status__c,&quot;Governance and Documentation Check Complete&quot;),
   NOT( Supporting_Documentation_Uploaded__c )
)</errorConditionFormula>
        <errorDisplayField>Supporting_Documentation_Uploaded__c</errorDisplayField>
        <errorMessage>Please ensure &quot;Supporting Documentation Uploaded&quot; is checked for a completed drawdown.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Drawdown_Utilisation</fullName>
        <active>true</active>
        <description>Draw down amount exceeds the  facility limit.</description>
        <errorConditionFormula>(Advance_Amount__c  +  Current_Utilisation__c ) &gt; Facility_Limit__c &amp;&amp;  ISPICKVAL(Status__c ,&apos;Governance and Documentation Check Complete&apos;) &amp;&amp;  RecordType.DeveloperName == &apos;Drawdown&apos; &amp;&amp; !ISPICKVAL( Approval_Status__c, &apos;Approved&apos;)</errorConditionFormula>
        <errorMessage>Approval required. Drawdown Amount cannot exceed the Facility Limit without Approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Interest_Period_Validation</fullName>
        <active>true</active>
        <description>Validation Rule to validate that both interest period and interest has been specified</description>
        <errorConditionFormula>OR
(
 AND( ISBLANK(TEXT(Interest_Period__c)), NOT(ISBLANK(Period__c))),
 AND( NOT(ISBLANK(TEXT(Interest_Period__c))), ISBLANK( Period__c ) )
)</errorConditionFormula>
        <errorMessage>Please ensure that the Period is specified when the Interest period is selected.
and the Interest Period is specified when the Period is populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lender_Portion_Approval_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Drawdown_Agency&apos; &amp;&amp;  (ISPICKVAL( Approval_Status__c , &apos;Submitted&apos;) || ISPICKVAL( Approval_Status__c , &apos;Rejected&apos;)) &amp;&amp;  ISPICKVAL(Status__c, &apos;Governance and Documentation Check Complete&apos;)</errorConditionFormula>
        <errorMessage>You are not allowed to complete the Drawdown without the Lender Portions being Approved</errorMessage>
    </validationRules>

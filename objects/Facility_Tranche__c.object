    <fields>
        <fullName>Availability_End_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
	ISPICKVAL(Availability__c,&apos;Day(s)&apos;),Availability_Start_Date__c + Availability_Period__c, IF( ISPICKVAL(Availability__c,&apos;Month(s)&apos;),
	 
DATE(
  YEAR( Availability_Start_Date__c) + FLOOR( ( MONTH ( Availability_Start_Date__c) + Availability_Period__c - 1 ) / 12 ),
  MOD( MONTH ( Availability_Start_Date__c) + Availability_Period__c - 1 + 
    IF( DAY ( Availability_Start_Date__c) &gt; CASE( MOD( MONTH( Availability_Start_Date__c) + Availability_Period__c - 1, 12 ) + 1, 
      2, 28,
      4, 30,
      6, 30,
      9, 30, 
      11, 30,
      31 ), 1, 0 ), 12 ) + 1,
    IF( DAY( Availability_Start_Date__c) &gt; CASE( MOD( MONTH( Availability_Start_Date__c ) + Availability_Period__c - 1, 12 ) + 1,
      2, 28, 
      4, 30, 
      6, 30, 
      9, 30, 
      11, 30, 
      31 ), 
    1, DAY( Availability_Start_Date__c)
  )
)






, DATE(YEAR(Availability_Start_Date__c)+Availability_Period__c, MONTH(Availability_Start_Date__c), DAY(Availability_Start_Date__c))



))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Availability End Date</label>
        <required>false</required>

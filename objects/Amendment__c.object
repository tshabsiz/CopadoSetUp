    <validationRules>
        <fullName>LIBRA_Documents_Uploaded_On_Amendment</fullName>
        <active>true</active>
        <description>Prompts CNSG to validate if the amendments documents have been uploaded.</description>
        <errorConditionFormula>IF(Amendment_Document_Uploaded__c = FALSE, TRUE, FALSE)</errorConditionFormula>
        <errorMessage>Please complete the Amendment Document Uploaded checkbox</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LIBRA_Other_Amendment_Reason_Type</fullName>
        <active>true</active>
        <description>Prompts CNSG user to complete the amendment reason field when &apos;other&apos; is selected on the the amendment reason type field.</description>
        <errorConditionFormula>AND(ISPICKVAL(Amendment_Reason_Type__c, &quot;Other&quot;) ,ISBLANK( 	Amendment_Reason__c))</errorConditionFormula>
        <errorMessage>Please complete the reason for the amendment?</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LIBRA_Regulatory_Compliance_Reason_Type</fullName>
        <active>true</active>
        <description>Prompts CNSG user to complete the regulation name and effective date when the reason is &quot;Regulatory Compliance&quot;.</description>
        <errorConditionFormula>AND(ISPICKVAL(Amendment_Reason_Type__c, &quot;Regulatory Compliance&quot;), (ISBLANK(Name_of_Regulation__c) &amp;&amp; ISBLANK(Effective_date__c)))</errorConditionFormula>
        <errorMessage>Please complete the Name of Regulation and Effective Date fields</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>

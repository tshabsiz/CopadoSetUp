    <validationRules>
        <fullName>Business_Unit_Mandatory_Check</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(Category__r.Allocated__c == true,  ISBLANK( Business_Unit_Cluster_Segment__c )), True, False)</errorConditionFormula>
        <errorDisplayField>Business_Unit__c</errorDisplayField>
        <errorMessage>Ensure that the Business Unit is captured.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cluster_Mandatory_Check</fullName>
        <active>false</active>
        <errorConditionFormula>IF(Category__r.Allocated__c =TRUE,ISPICKVAL(Cluster__c,&quot;&quot;),FALSE)</errorConditionFormula>
        <errorDisplayField>Cluster__c</errorDisplayField>
        <errorMessage>Ensure that the cluster is captured.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_2_Week_Entry_Window</fullName>
        <active>false</active>
        <description>You may log entries for 2 weeks prior of the current date.</description>
        <errorConditionFormula>TODAY() - advpm__Entry_Date__c &gt;= 14</errorConditionFormula>
        <errorDisplayField>advpm__Entry_Date__c</errorDisplayField>
        <errorMessage>You may log entries for 2 weeks prior of the current date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_Time_Entries_for_2_Week_Window</fullName>
        <active>false</active>
        <description>You may not edit time entries that were created 2 weeks prior to the current date.</description>
        <errorConditionFormula>TODAY() - DATEVALUE(CreatedDate) &gt;= 14</errorConditionFormula>
        <errorMessage>You may not edit time entries that were created 2 weeks prior to the current date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_capturing_of_Negative_Time_entry</fullName>
        <active>true</active>
        <description>[W-000386] - User must not be able to capture negative time records.</description>
        <errorConditionFormula>advpm__Time_In_Hours__c  &lt; 0</errorConditionFormula>
        <errorDisplayField>advpm__Time_In_Hours__c</errorDisplayField>
        <errorMessage>Hours Worked cannot be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Specify_A_Matter</fullName>
        <active>true</active>
        <description>You must specify a Matter / Deal unless you indicate the entry is Non-matter Related.</description>
        <errorConditionFormula>IF(AND(Category__r.Allocated__c == true,  ISBLANK(advpm__Matter__c)), True, False)</errorConditionFormula>
        <errorDisplayField>advpm__Matter__c</errorDisplayField>
        <errorMessage>You must either indicate the related &apos;Matter / Deal&apos; OR mark the entry as &apos;Non-matter Related&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Segment_Mandatory_Check</fullName>
        <active>true</active>
        <errorConditionFormula>IF( NOT(ISPICKVAL(Cluster__c,&quot;&quot;))&amp;&amp; (Category__r.Allocated__c =TRUE),ISPICKVAL(Segment__c,&quot;&quot;),FALSE)</errorConditionFormula>
        <errorDisplayField>Segment__c</errorDisplayField>
        <errorMessage>Ensure that the Segment is captured.</errorMessage>
    </validationRules>

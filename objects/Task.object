    <webLinks>
        <fullName>advpm__Mass_Edit_Update</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mass Edit/Update</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>var records = {!GETRECORDIDS( $ObjectType.Task )};
var msg = &apos;&apos;;

if( records.length &gt; 100 ) { 
    msg = &apos;You should not select more than 100 records at a time&apos;;
}
if( records.length &lt; 1 ){
   msg = &apos;Please select at least 1 record&apos;; 
}
var hasEvents = false;
var hasTasks = false;
for( i=0; i&lt;records.length; i++ )
{
	var idVal = records[i];
	if ( idVal.substring(0,3) == &apos;{!$ObjectType.Task}&apos; )
	{
		hasTasks = true;
	}
	if ( idVal.substring(0,3) == &apos;{!$ObjectType.Event}&apos; )
	{
		hasEvents = true;
	}
}
if (hasEvents == true &amp;&amp; hasTasks == true)
{
	msg = &apos;You cannot edit both Events and Tasks at the same time. Please either select All Tasks or All Events.&apos;;
}

if( msg != &apos;&apos; ) {
	alert(msg);    
}
else{
	location.href = &apos;{!$Site.Prefix}/apex/advpm__MassUpdateObject?recIds=&apos;+records;
}</url>
    </webLinks>

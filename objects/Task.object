    <validationRules>
        <fullName>Collateral_Valuation</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
    RecordType.Name = &quot;Collateral Valuation&quot;,
    WhatId  &lt;&gt; &quot;01I9E000000Cmxp&quot;
)</errorConditionFormula>
        <errorMessage>Please ensure that the task is related to a Collateral.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Initial_Valuation_Date_Is_Not_Blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNEW()), OR (
(Subject = &quot;Perform Valuation&quot;), Subject = &quot;Create Report&quot;, Subject = &quot;Review Report&quot;, Subject = &quot;Finalised Report&quot;),  
(NOT(ISBLANK( Initial_Valuation_Date__c  )) ) 
)</errorConditionFormula>
        <errorDisplayField>Initial_Valuation_Date__c</errorDisplayField>
        <errorMessage>You may only specify the Actual Completion Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Initial_Valuation_Date_must_be_Specified</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNEW()),
(Subject = &quot;Contact Client to Schedule Valuation&quot;),
(ISBLANK(Initial_Valuation_Date__c ) )
)</errorConditionFormula>
        <errorDisplayField>Initial_Valuation_Date__c</errorDisplayField>
        <errorMessage>Please ensure that you have specified a Valuation Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Specify_Actual_Completion_Date</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() = FALSE &amp;&amp;RecordType.DeveloperName == &quot;Collateral_Valuation&quot; &amp;&amp;  (Description == NULL || ISBLANK(Actual_Completion_Date__c) = TRUE) &amp;&amp; Subject != &quot;Contact Client to Schedule Valuation&quot;</errorConditionFormula>
        <errorMessage>Please ensure that you have specified an Actual Completion Date and Comments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Reason_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Status, &apos;Not Taken Up&apos;) &amp;&amp; ISBLANK(TEXT(Status_Reason__c)) &amp;&amp;
OR(ISBLANK(RecordType.DeveloperName),RecordType.DeveloperName = &quot;Default_Task&quot;,RecordType.DeveloperName = &quot;Campaign_Task&quot;)</errorConditionFormula>
        <errorDisplayField>Status_Reason__c</errorDisplayField>
        <errorMessage>Status Reason required for this Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>advpm__Validate_Time_Log</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
	advpm__Log_Time__c == true, 
	OR (
		ISBLANK( advpm__Time_In_Hours__c ), 
		ISBLANK( WhatId )
	) 
)</errorConditionFormula>
        <errorDisplayField>WhatId</errorDisplayField>
        <errorMessage>Log Time requires &apos;Related To&apos; and &apos;Time (In Hours)&apos; value be provided.</errorMessage>
    </validationRules>

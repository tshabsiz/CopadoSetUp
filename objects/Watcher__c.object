    <validationRules>
        <fullName>Specify_Monitoring_Comment</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Result__c ,&quot;Expectation Not Met&quot;),
Comments__c == NULL
)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Please ensure that you have added a comment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Specify_Valuation_Comment</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(
ISPICKVAL( Report_Grading__c ,&apos;Concern&apos;) , ISPICKVAL( Report_Grading__c ,&apos;Escalation&apos;)),  Comments__c == NULL)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Please ensure that you have added a comment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>System_Generated_Status</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISCHANGED(Watcher_Status__c) == TRUE &amp;&amp; (ISPICKVAL(Watcher_Status__c,&apos;New&apos;) || ISPICKVAL(Watcher_Status__c,&apos;Due&apos;) || ISPICKVAL(Watcher_Status__c,&apos;Overdue&apos;)), True, False)</errorConditionFormula>
        <errorDisplayField>Watcher_Status__c</errorDisplayField>
        <errorMessage>You are not allowed to change the Watcher Status to a System Generated Status (New, Due, Overdue)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_Notification_User</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Watcher_Status__c , &quot;Breached with Action&quot;) &amp;&amp;  ISBLANK( Task_Notification_User__c ) &amp;&amp;  RecordType.DeveloperName = &quot;Trigger&quot;</errorConditionFormula>
        <errorDisplayField>Task_Notification_User__c</errorDisplayField>
        <errorMessage>Breached With Action! Please select a Participant to notify</errorMessage>
    </validationRules>

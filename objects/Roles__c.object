        <fullName>Prevent_Disable_Primary_Role</fullName>
        <active>true</active>
        <description>Prevents user from unchecking the Primary Role check box</description>
        <errorConditionFormula>AND (
 Apex_Context__c  = false,
 ISCHANGED(Primary_Role__c),
 Primary_Role__c = false
)</errorConditionFormula>

    <webLinks>
        <fullName>New_Association</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>New Role</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>/a4y/e?CF00N8E000000G8Ue={!Contact.Name}&amp;Name={!Contact.FirstName}%20{!Contact.LastName}&amp;CF00N2400000IFFo3_lkid={!Contact.Id}&amp;CF00N2400000IFFo3={!Contact.Name}&amp;retURL=%2F{!Contact.Id}</url>
    </webLinks>
    <webLinks>
        <fullName>New_Contact</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Contact</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>var ownerName = &apos;{!Account.OwnerFullName}&apos;; 
var businessSegment = &apos;{!Account.Segment__c}&apos;; 

if(ownerName==&apos;Coverage Unmanaged&apos; &amp;&amp; businessSegment!=&apos;Business Banking&apos;) 
{ 
alert(&apos;Contacts have been disabled for Relationships owned by Coverage Unmanaged &apos;); 
} 
else 
{ 
location.href = &apos;/003/e?con4={!Account.Name}&amp;con4_lkid={!Account.Id}&apos;; 
}</url>
    </webLinks>
    <webLinks>
        <fullName>New_Contact_From_Client</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Contact</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>var ownerName = &apos;{!Account.OwnerFullName}&apos;; 
var businessSegment = &apos;{!Account.Segment__c}&apos;; 

if(ownerName==&apos;Africa RD Holder&apos; &amp;&amp; businessSegment!=&apos;Business Banking&apos;) 
{ 
alert(&apos;Contacts have been disabled for Relationships owned by Africa RD Holder &apos;); 
} 
else 
{ 
location = &apos;/003/e?con4={!Account.Parent}&amp;con4_lkid={!Account.ParentId}&amp;CF00N2400000ISxBm_lkid={!Account.Id}&amp;CF00N2400000ISxBm={!Account.Name}&amp;retURL={!Account.Id}&apos;; 
}</url>
    </webLinks>

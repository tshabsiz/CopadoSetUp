        NOT(ISBLANK(TEXT(Date__c))),
        NOT(ISBLANK(Number_Of_Days__c)),
        NOT(ISBLANK(TEXT(Days__c)))
     )
)</errorConditionFormula>
        <errorMessage>Please ensure only a &quot;Due Date&quot; is specified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TMG_Once_Off_Fields</fullName>
        <active>true</active>
        <description>Once Off Scheduling</description>
        <errorConditionFormula>AND
(
OR(Active__c,  Activate_Later__c ),
ISPICKVAL(Recurrence__c, &quot;Once Off&quot;),
ISBLANK(TEXT(Due_Date__c))
)</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>You must provide a &quot;Due Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TMG_Once_Off_New_Past</fullName>
        <active>true</active>
        <description>You may not create a &quot;Once Off&quot; condition with a &quot;Due Date&quot; in the past.</description>
        <errorConditionFormula>AND
(
ISNEW(),
NOT(ISBLANK(Due_Date__c)),
Due_Date__c &lt; TODAY(),
Active__c
)</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>You may not create a &quot;Once Off&quot; condition with a &quot;Due Date&quot; in the past.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TMG_Other_Relative_Date_Ensure_Blank</fullName>
        <active>true</active>
        <description>You may NOT specify a &quot;Due Date&quot;.</description>
        <errorConditionFormula>AND
(
     Active__c,
     NOT(ISPICKVAL(Recurrence__c, &quot;Once Off&quot;)),
     ISPICKVAL(Date__c,&quot;Other&quot;),
     OR
     (
        NOT(ISBLANK(TEXT( Due_Date__c )))
     )
)</errorConditionFormula>
        <errorMessage>You may NOT specify a &quot;Due Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TMG_Other_Relative_Date_Fields</fullName>
        <active>true</active>
        <description>You must provide an &quot;Other Relative Date&quot;.</description>
        <errorConditionFormula>AND
(
     OR(Active__c, Activate_Later__c ),
     NOT(ISPICKVAL(Recurrence__c, &quot;Once Off&quot;)),
     ISPICKVAL(Date__c,&quot;Other&quot;),
     ISBLANK(TEXT(Other_Relative_Date__c ))
)</errorConditionFormula>
        <errorDisplayField>Other_Relative_Date__c</errorDisplayField>
        <errorMessage>You must provide an &quot;Other Relative Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TMG_Relative_Date_Ensure_Blank</fullName>
        <active>true</active>
        <description>You may NOT specify a &quot;Due Date&quot; or &quot;Other Relative Date&quot;.</description>
        <errorConditionFormula>AND
(
     Active__c,
     NOT(ISPICKVAL(Recurrence__c, &quot;Once Off&quot;)),
     NOT(ISPICKVAL( Date__c , &quot;Other&quot;)),
     OR
     (
        NOT(ISBLANK(TEXT( Due_Date__c ))),
        NOT(ISBLANK(TEXT( Other_Relative_Date__c)))
     )
)</errorConditionFormula>
        <errorMessage>You may NOT specify a &quot;Due Date&quot; or &quot;Other Relative Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TMG_Relative_Date_Fields</fullName>
        <active>true</active>
        <description>You must provide a &quot;Relative Date&quot;, &quot;Number of Days&quot; and &quot;Days&quot;.</description>
        <errorConditionFormula>AND
(
     OR(Active__c, Activate_Later__c ),
     NOT(ISPICKVAL(Recurrence__c, &quot;Once Off&quot;)),
     NOT(ISBLANK(TEXT(Recurrence__c))),
     OR(
        ISBLANK(TEXT(Date__c)),
        ISBLANK(Number_Of_Days__c),
        ISBLANK(TEXT(Days__c))
     )
)</errorConditionFormula>
        <errorMessage>You must provide a &quot;Relative Date&quot;, &quot;Number of Days&quot; and &quot;Days&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Threshold_Category_Mandatory_Check</fullName>
        <active>true</active>
        <errorConditionFormula>IF (OR(ISPICKVAL(Threshold_Type__c, &quot;Base&quot;),ISPICKVAL(Threshold_Type__c, &quot;Base Range&quot;)),ISPICKVAL( Threshold_Category__c ,&quot;&quot;),FALSE)</errorConditionFormula>
        <errorDisplayField>Threshold_Category__c</errorDisplayField>
        <errorMessage>Capture the Threshold Category</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Threshold_Comparator_Mandatory_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Threshold_Type__c , &quot;Base&quot;)&amp;&amp;(ISPICKVAL( Threshold_Category__c , &quot;Ratio&quot;)||(ISPICKVAL(Threshold_Category__c, &quot;Percentage&quot;))||(ISPICKVAL(Threshold_Category__c, &quot;Multiple&quot;))),ISBLANK(TEXT( Threshold_Comparator__c )))</errorConditionFormula>
        <errorDisplayField>Threshold_Comparator__c</errorDisplayField>
        <errorMessage>Capture the Threshold Comparator</errorMessage>
    </validationRules>

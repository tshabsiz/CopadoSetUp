		ISBLANK(Due_Date__c),
		NOT(ISBLANK(TEXT(Days__c))),
		NOT(ISBLANK(TEXT(Date__c)))
	),
	AND
	(
		TEXT(Recurrence__c ) == &apos;Once Off&apos;,
		NOT(ISBLANK(Due_Date__c)),
		ISBLANK(TEXT(Date__c))
	),
	
	AND
	(
		TEXT(Recurrence__c ) != &apos;Once Off&apos;,
			NOT(ISBLANK(Number_Of_Days__c)),
		ISBLANK(Due_Date__c),
		NOT(ISBLANK(TEXT(Days__c))),
		NOT(ISBLANK(TEXT(Date__c))),
		AND
		(
			IF
			(
				TEXT(Date__c) == &apos;Signature Date&apos; &amp;&amp; NOT(ISBLANK(Signature_Date__c)),
				true,
				IF
				(
					TEXT(Date__c) == &apos;Effective Date&apos; &amp;&amp; NOT(ISBLANK(Effective_Date__c)),
					true,
					false
				)
			)
		)
	)
)</formula>

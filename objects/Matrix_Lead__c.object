    <webLinks>
        <fullName>Convert_to_Prospect</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Convert to Prospect</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot;)}

if({!ISBLANK(Matrix_Lead__c.Company_Name__c) || ISBLANK(Matrix_Lead__c.Surname__c)}){
  alert(&quot;Matrix Lead cannot be converted. Company Name and Surname are required fields.&quot; );
} else if({!NOT(ISPICKVAL(Matrix_Lead__c.Lead_Status__c, &apos;Contacted&apos;))}){
  alert(&quot;Error: Only Matrix Leads in status Contacted can be converted.&quot; );
} else if(window.confirm(&quot;Are you sure?&quot;)){
  var result = sforce.apex.execute(&quot;MatrixLeadConversion&quot;, &quot;convertMatrixLeadToProspect&quot;, {matrixLeadId:&quot;{!Matrix_Lead__c.Id}&quot;})+&apos;&apos;;
  if(result.indexOf(&apos;ERROR&apos;) &gt;= 0){
    alert(result);
  } else{
    window.open(&quot;/&quot;+result, &apos;_self&apos;);
  }
}</url>
    </webLinks>

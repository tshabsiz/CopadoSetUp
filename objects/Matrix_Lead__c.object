    <validationRules>
        <fullName>Contacted_or_Unqualified_Leads</fullName>
        <active>true</active>
        <description>If leads status has been changed to &quot;Contacted&quot; or &quot;Unqalified,a reason must be provided</description>
        <errorConditionFormula>ISPICKVAL( Lead_Status__c , &quot;Contacted&quot;)&amp;&amp;  ISBLANK(Comments__c)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>Please provide a comment</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Converted_Leads</fullName>
        <active>true</active>
        <description>Display error message</description>
        <errorConditionFormula>ISPICKVAL(Lead_Status__c,&quot;Converted&quot;)&amp;&amp; NOT(ISCHANGED(Lead_Status__c))</errorConditionFormula>
        <errorDisplayField>Lead_Status__c</errorDisplayField>
        <errorMessage>Converted Leads cannot be updated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pending_Assignment_Leads_are_read_only</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE(Lead_Status__c), &apos;Pending Assignment&apos;) &amp;&amp;  NOT(ISPICKVAL(Lead_Status__c, &apos;Assigned&apos;) || ISPICKVAL(Lead_Status__c, &apos;Open&apos;))</errorConditionFormula>
        <errorMessage>Leads in status &quot;Pending Assignment&quot; cannot be updated until they are approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_for_unqualified_leads</fullName>
        <active>true</active>
        <description>Reason must be provided why the lead was unqualified</description>
        <errorConditionFormula>ISPICKVAL( Lead_Status__c ,&quot;Unqualified&quot;)&amp;&amp; ISBLANK(TEXT(Reason_for_unqualified_leads__c))</errorConditionFormula>
        <errorDisplayField>Reason_for_unqualified_leads__c</errorDisplayField>
        <errorMessage>Please select a reason for unqualified lead</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Specify_Other</fullName>
        <active>true</active>
        <description>Specify why the lead is unqualified</description>
        <errorConditionFormula>ISPICKVAL(Lead_Status__c, &apos;Unqualified&apos;) &amp;&amp;  
ISPICKVAL(Reason_for_unqualified_leads__c ,&apos;Other(please specify)&apos;) 
&amp;&amp; ISBLANK(Specify_Other__c)</errorConditionFormula>
        <errorDisplayField>Specify_Other__c</errorDisplayField>
        <errorMessage>Please specify other reason for lead to be unqualified</errorMessage>
    </validationRules>

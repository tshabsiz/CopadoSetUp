    <webLinks>
        <fullName>Submit_Request_for_Quotes</fullName>
        <availability>online</availability>
        <description>Submits the Legal Quote Request to the specified Law Firms.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit Request for Quotes</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/apex.js&quot;)} 

var confirmation = confirm(&quot;Are you sure you want to submit the request for quotes?&quot;);

if (confirmation == true) 
{
	var soqlQuery = sforce.connection.query(&quot;SELECT Id, Status__c, Quote_Approver__c FROM Legal_Quote_Request__c WHERE Id =&apos;{!Legal_Quote_Request__c.Id}&apos; LIMIT 1&quot;);
	var records = soqlQuery.getArray(&quot;records&quot;); 
	var quoteApprover = records[0].Quote_Approver__c;
	var currentStatus = records[0].Status__c;

	if(quoteApprover == &apos;&apos; || quoteApprover == null)
	{
		alert(&quot;There is no Quote Approver assigned to this request.&quot;);   
	}
	else
	{

		if(currentStatus == &apos;Draft&apos;)
		{
			//Identify the record.
			var legalQuoteRequest = new sforce.SObject(&quot;Legal_Quote_Request__c&quot;);

			//Set object to current record.
			legalQuoteRequest.Id = &quot;{!Legal_Quote_Request__c.Id}&quot;;

			//Make the field change
			legalQuoteRequest.Status__c = &quot;Requested&quot;;

			//save the change
			sforce.connection.update([legalQuoteRequest]);			

			//refresh the page
			window.location.reload();
			window.location.reload();
		}
		else
		{
			alert(&quot;This quote request has already been requested.&quot;);
		}
	}
} 
else 
{
	//Do nothing.
}</url>

    <fields>
        <fullName>Matter_Value__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
  Matter__r.Total_Claim_Value_New__c &gt;= 0 &amp;&amp; Matter__r.Total_Claim_Value_New__c &lt;= 30000, &quot;Very Low&quot;,
  IF(Matter__r.Total_Claim_Value_New__c&gt; 30000 &amp;&amp; Matter__r.Total_Claim_Value_New__c &lt;= 300000, &quot;Low&quot;,
  IF(Matter__r.Total_Claim_Value_New__c&gt; 300000 &amp;&amp; Matter__r.Total_Claim_Value_New__c&lt;= 5000000, &quot;Medium&quot;,
  IF(Matter__r.Total_Claim_Value_New__c&gt; 500000 &amp;&amp; Matter__r.Total_Claim_Value_New__c &lt;= 1000000, &quot;High&quot;,
  &quot;Very High&quot;))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>

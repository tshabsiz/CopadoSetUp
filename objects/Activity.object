    <fields>
        <fullName>Status_Icon__c</fullName>
        <externalId>false</externalId>
        <formula>IF(IsTask,
  IF(IsClosed,
    IMAGE(&quot;/servlet/servlet.FileDownload?file=01524000000d3Ew&quot;, &quot;Green&quot;, 16, 16),
    IF(ActivityDate &gt;= TODAY(), 
      IMAGE(&quot;/servlet/servlet.FileDownload?file=01524000000d3Ey&quot;, &quot;Yellow&quot;, 16, 16) , 
      IMAGE(&quot;/servlet/servlet.FileDownload?file=01524000000d3Ex&quot;, &quot;Red&quot;, 16, 16) 
    ) 
  ),
  null
)</formula>
        <label>Status</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>

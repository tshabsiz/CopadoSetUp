    <validationRules>
        <fullName>ReasonRequireWhenOtherSelectedInStatus</fullName>
        <active>true</active>
        <description>Validates that a custom field called Reason contains a value
if a campaign member status has a Reason of “Not Interested -Other.”</description>
        <errorConditionFormula>AND(
TEXT(Status) = &apos;Not Interested&apos;,
ISBLANK(Reason__c))</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>Please enter a reason</errorMessage>
    </validationRules>

    <webLinks>
        <fullName>Cancel_Document</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel Document</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/39.0/apex.js&quot;)} 

var confirmation = confirm(&quot;Are you sure you want to cancel this document?&quot;);

if(confirmation == true) 
{
    var soqlQuery = sforce.connection.query(&quot;SELECT Id, Status__c FROM Matter_Spend_Document__c WHERE Id =&apos;{!Matter_Spend_Document__c.Id}&apos; LIMIT 1&quot;);
	var records = soqlQuery.getArray(&quot;records&quot;); 
	var currentStatus = records[0].Status__c;

	if(currentStatus == &apos;Queried&apos; || currentStatus == &apos;New&apos; || currentStatus == &apos;Ready For Payment&apos;)
	{		
		//Identify the record.
		var spendDocument = new sforce.SObject(&quot;Matter_Spend_Document__c&quot;);

		//Set object to current record.
		spendDocument.Id = &quot;{!Matter_Spend_Document__c.Id}&quot;;

		//Make the field change
		spendDocument.Status__c = &quot;Cancelled&quot;;

		//save the change
		sforce.connection.update([spendDocument]);			

		//refresh the page
		window.location.reload();
	}
	else
	{
		alert(&quot;You may not cancel a document in this status.&quot;);
	}
}</url>

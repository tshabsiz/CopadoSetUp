    <fields>
        <fullName>My_Call_Report__c</fullName>
        <description>Set the user ID to 1 if the Absa Attendee is the same as the user logged in (Running User)</description>
        <externalId>false</externalId>
        <formula>IF( OR( Barclays_Attendee__r.Id =  $User.Id ), 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>My Call Report</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Senior_Sponsor__c</fullName>
        <externalId>false</externalId>
        <formula>OR(Barclays_Attendee__r.Senior_Sponsor__c,Call_Report__r.Owner:User.Senior_Sponsor__c)</formula>
        <label>Senior Sponsor</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>

    <validationRules>
        <fullName>Values_In_Barc_Employee_and_Non_Employee</fullName>
        <active>true</active>
        <description>Prevents users from entering a value in the Barclays Employee and Non-Employee</description>
        <errorConditionFormula>(ISBLANK( Barclays_Attendee__c ) = FALSE )&amp;&amp;
(ISBLANK(  Client_Attendee__c  )= FALSE)</errorConditionFormula>
        <errorDisplayField>Barclays_Attendee__c</errorDisplayField>
        <errorMessage>A record can only contain either an employee or a non-employee , please create a new attendee record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>team_member_confirmation</fullName>
        <active>true</active>
        <description>This rule checks whether the  &quot;I confirm team member has been cleared&quot; field is checked.</description>
        <errorConditionFormula>AND (
 RecordType.Name = &apos;Shared with&apos;,
 I_confirm_team_member_has_been_cleared__c = FALSE
)</errorConditionFormula>
        <errorDisplayField>I_confirm_team_member_has_been_cleared__c</errorDisplayField>
        <errorMessage>Please make sure that you have checked the &quot;I confirm team member has been cleared&quot; field.</errorMessage>
    </validationRules>

    <fields>
        <fullName>Attendee_Email__c</fullName>
        <externalId>false</externalId>
        <formula>IF(NOT(ISBLANK(Barclays_Attendee__c)), 
  Barclays_Attendee__r.Email,  
  IF(NOT(ISBLANK(Client_Attendee__c)), 
    Client_Attendee__r.Email, 
    &apos;&apos;
  )
)</formula>
        <label>Attendee Email</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Attendee_Name__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK( Barclays_Attendee__c )= FALSE ,
 Barclays_Attendee__r.FirstName&amp;&quot; &quot;&amp;Barclays_Attendee__r.LastName,IF(ISBLANK( Client_Attendee__c )= FALSE ,
 Client_Attendee__r.FirstName&amp;&quot; &quot;&amp;Client_Attendee__r.LastName,IF(ISBLANK( Wealth_Attendee__c )= FALSE ,
   HYPERLINK(&quot;/&quot;&amp;Wealth_Attendee__r.Id , Wealth_Attendee__r.Name  , &quot;_self&quot;)   ,&apos;&apos;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Attendee Name</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Attendee_Type__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK( Barclays_Attendee__c )= FALSE ,
 HYPERLINK(&quot;/&quot;&amp;Id , &quot;Barclays Attendee&quot;,&quot;_Self&quot;),
IF(ISBLANK( Client_Attendee__c )= FALSE ,
HYPERLINK(&quot;/&quot;&amp;Id , &quot;Client Attendee&quot;,&quot;_Self&quot;),
IF(ISBLANK( Wealth_Attendee__c )= FALSE ,
 HYPERLINK(&quot;/&quot;&amp;Id , &quot;Role Attendee&quot;,&quot;_Self&quot;),&apos;&apos;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Attendee Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>

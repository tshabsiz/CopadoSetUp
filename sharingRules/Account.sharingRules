        <accessLevel>Read</accessLevel>
        <accountSettings>
            <caseAccessLevel>Read</caseAccessLevel>
            <contactAccessLevel>Read</contactAccessLevel>
            <opportunityAccessLevel>None</opportunityAccessLevel>
        </accountSettings>
        <description>Wimi users should see   EDC Client record type but only the segment Multi-Manager</description>
        <label>Accounts shared with WIMI users</label>
        <sharedTo>
            <roleAndSubordinatesInternal>Institutional_Clients</roleAndSubordinatesInternal>
        </sharedTo>
        <booleanFilter>(1 AND 2) OR 3</booleanFilter>

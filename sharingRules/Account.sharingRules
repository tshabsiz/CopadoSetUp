            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>EDC Client</value>
        </criteriaItems>
        <criteriaItems>
            <field>Segment__c</field>
            <operation>equals</operation>
            <value>Multi-manager,Asset Consultants</value>
        </criteriaItems>
        <criteriaItems>

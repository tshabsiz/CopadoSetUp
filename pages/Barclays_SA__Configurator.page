            // aren't multiple calls scheduled at the same time.
            var saveSubCagetoryTimedFunction = function() {
                
                //  If we've already scheduled a call, we cancel it first...
                if(saveSubCategoryTimer) {
                    clearTimeout(saveSubCategoryTimer);

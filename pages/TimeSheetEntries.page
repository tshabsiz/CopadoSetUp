    <script>
        function checkOnLeave(elem) {
            var categoryName = document.getElementById(elem.name);
            var selectedValue = categoryName.options[categoryName.selectedIndex].text;
            if (selectedValue == "Out of Office")
            {
                //Find the closest column next to category and disable it if 'out of office' is selected
                var matName = document.getElementById(j$(elem).closest('tr').find("input[id$='matterName']").attr('id'));
                matName.disabled = true;
                                
                //Find the closest column next to matter name and disable it if 'out of office' is selected
                var busUnit = document.getElementById(j$(matName).closest('tr').find("input[id$='BusinessUnitInput']").attr('id'));
                busUnit.disabled = true;
                
                //Find the closest column next to business unit and disable it if 'out of office' is selected
                var descField = document.getElementById(j$(busUnit).closest('tr').find("textarea[id$='DescriptionField']").attr('id'));
                descField.disabled = true;
                
                //disable icons
                document.getElementById(j$(elem).closest('tr').find("a[id$='matterName_lkwgt']").attr('id')).style.visibility = 'hidden';
				document.getElementById(j$(matName).closest('tr').find("a[id$='BusinessUnitInput_lkwgt']").attr('id')).style.visibility = 'hidden';
            }
            else{
                resetOutOfOfficeFunction(elem);
            }
        }
    
    	//after refresh happens previuous 'out of office' matter name and description fields are no longer greyed out
    	//this function checks this and greys them out
   		function checkAllRowsOnLeave() {
            $("tr.dataRow").each(function() {
                $this = $(this);
                var test = document.getElementById(j$(this).closest('tr').find("select[id$='CategoryList']").attr('id')).id;
                var categoryName = document.getElementById(test);
                var selectedValue = categoryName.options[categoryName.selectedIndex].text;
                if (selectedValue == "Out of Office")
                {
                    //Find the closest column next to category and disable it if 'out of office' is selected
                    var matName = document.getElementById(j$(this).closest('tr').find("input[id$='matterName']").attr('id'));
                    matName.disabled = true;
                    //Find the closest column next to matter name and disable it if 'out of office' is selected
                    var busUnit = document.getElementById(j$(this).closest('tr').find("input[id$='BusinessUnitInput']").attr('id'));
                    busUnit.disabled = true;
                    
                    //Find the closest column next to business unit and disable it if 'out of office' is selected
                    var descField = document.getElementById(j$(this).closest('tr').find("textarea[id$='DescriptionField']").attr('id'));
                    descField.disabled = true;
                    
                    //disable icons
                	document.getElementById(j$(this).closest('tr').find("a[id$='matterName_lkwgt']").attr('id')).style.visibility = 'hidden';
					document.getElementById(j$(this).closest('tr').find("a[id$='BusinessUnitInput_lkwgt']").attr('id')).style.visibility = 'hidden';
                }
            });
        }

        function resetOutOfOfficeFunction(elem) {
            var categoryName = document.getElementById(elem.name);
            var selectedValue = categoryName.options[categoryName.selectedIndex].text;

            //Find the closest column next to category and disable it if 'out of office' is selected
            var matName = document.getElementById(j$(elem).closest('tr').find("input[id$='matterName']").attr('id'));
            matName.disabled = false;

            //Find the closest column next to matter name and disable it if 'out of office' is selected
            var busUnit = document.getElementById(j$(matName).closest('tr').find("input[id$='BusinessUnitInput']").attr('id'));
            busUnit.disabled = false;

            //Find the closest column next to business unit and disable it if 'out of office' is selected
            var descField = document.getElementById(j$(busUnit).closest('tr').find("textarea[id$='DescriptionField']").attr('id'));
            descField.disabled = false;

            //disable icons
            document.getElementById(j$(elem).closest('tr').find("a[id$='matterName_lkwgt']").attr('id')).style.visibility = 'visible';
            document.getElementById(j$(matName).closest('tr').find("a[id$='BusinessUnitInput_lkwgt']").attr('id')).style.visibility = 'visible';

        }
    </script>
    
    <script>
        function hoursValidator(hours) {
            //check if time entered is greater than 24 hours
            var enteredValue = hours.value;
            if(enteredValue > 24)
            {
                //Give alert that invalid time
                alert("Invalid time entered! Value must be less than 24 hours!");
                alterHours(hours);
            }

            //Convert string to float  for calculation
            enteredValue = parseFloat(enteredValue);

            //Check if total time for day is greater than 24 hours
            if(hours.id.includes("MonField")){
                if(({!totalMonday} + enteredValue) > 24){
                    alert("Total time entered for day exceeds 24 hours!");
                    alterHours(hours);
                }
            }
            if(hours.id.includes("TueField")){
                if(({!totalTuesday} + enteredValue) > 24){
                    alert("Total time entered for day exceeds 24 hours!");
                    alterHours(hours);
                }
            }
            if(hours.id.includes("WedField")){
                if(({!totalWednesday} + enteredValue) > 24){
                    alert("Total time entered for day exceeds 24 hours!");
                    alterHours(hours);
                }
            }
            if(hours.id.includes("ThuField")){
                if(({!totalThursday} + enteredValue) > 24){
                    alert("Total time entered for day exceeds 24 hours!");
                    alterHours(hours);
                }
            }
            if(hours.id.includes("FriField")){
                if(({!totalFriday} + enteredValue) > 24){
                    alert("Total time entered for day exceeds 24 hours!");
                    alterHours(hours);
                }
            }
            if(hours.id.includes("SatField")){
                if(({!totalSaturday} + enteredValue) > 24){
                    alert("Total time entered for day exceeds 24 hours!");
                    alterHours(hours);
                }
            }
            if(hours.id.includes("SunField")){
                if(({!totalSunday} + enteredValue) > 24){
                    alert("Total time entered for day exceeds 24 hours!");
                    alterHours(hours);
                }
            }
        }

        function alterHours(hours){
            //get current input field reference's id
            var hoursId = document.getElementById(hours.getAttribute('id')).id;
            //set input back to '0.00'
            var resetTime = document.getElementById(hoursId);
            resetTime.value = "0.00";

            //reset total
            //get current total value
            var totTime = document.getElementById(j$(hours).closest('tr').find("span[id$='t']").attr('id')).id;
            var timeValue = document.getElementById(totTime).innerText;
            //subtract total value from invalid entered value
            var resetTotal = (timeValue - (enteredValue));
            //set total back to valid total
            document.getElementById(totTime).innerText = resetTotal;
        }
    </script>
    

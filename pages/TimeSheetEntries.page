<script>
    var j$ = jQuery.noConflict();

    j$(document).ready(function() {

        //Autocomplete
        j$(document.body).on('focus', 'input.searchAutocomplete' ,function(){

            var map = new Map();

            j$(this).autocomplete({
                source: function (request, response)
                {
                    Visualforce.remoting.Manager.invokeAction(
                        '{!$RemoteAction.TimeSheetEntries_Cont.searchMatters}',
                        request.term, 
                        function(result, event)
                        {
                            var object = [];

                            j$.each(result, function(key, value){
                                object.push(value);
                                map.set(value.buValue, value.buLabel);
                            });

                            response(object);
                        }, 
                        {
                            escape: false
                        }
                    );                    
                },
                minLength: 3,
                delay: 200,
                select: function(event, ui) 
                {
                    event.preventDefault();
                    j$(this).val(ui.item.label);
                    j$(this).closest('tr').find("input[id$='thisRowsMatterId']:hidden:first").val(ui.item.value);

  
                    console.log(map);
                }
            });
        });
    });
    </script>
    

<apex:page standardController="advpm__Matter__c" showHeader="{!IF(JSINHTMLENCODE($CurrentPage.parameters.nohead)=='1','false','true')}" sidebar="false" title="Matter Gantt">
    <apex:includeScript value="{!URLFOR($Resource.advpm__jQueryZip, 'js/jquery-1.8.3.min.js')}" />
    <iframe id="iframe_panel" frameborder="0" height="825px" scrolling="no" title="Matter Gantt" width="100%" src="about:blank"></iframe>
    <script type="text/javascript">
        $j = jQuery.noConflict();
        $j(document).ready(function () {
            if ( (typeof sforce != 'undefined') && (sforce != null) ) {
                sforce.one.navigateToURL('{!$Site.Prefix}/apex/advpm__MatterGantt_Main?id={!JSINHTMLENCODE($CurrentPage.parameters.id)}&nohead=1&cardwall={!JSINHTMLENCODE($CurrentPage.parameters.cardwall)}&p={!JSINHTMLENCODE($CurrentPage.parameters.p)}');
            }
            else {
                $j("#iframe_panel").contents().empty();
                $j("#iframe_panel").attr("src", "{!$Site.Prefix}/apex/advpm__MatterGantt_Main?id={!JSINHTMLENCODE($CurrentPage.parameters.id)}&nohead=1&cardwall={!JSINHTMLENCODE($CurrentPage.parameters.cardwall)}&p={!JSINHTMLENCODE($CurrentPage.parameters.p)}");
            }
        });
    </script>
</apex:page>
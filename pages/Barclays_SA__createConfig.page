<apex:page standardController="Barclays_SA__MobileAppConfig__c" extensions="Barclays_SA.CreateConfig">
	<head>
		<style type="text/css">
			body {
				background-image:url('{!URLFOR($Resource.dsav3_imx, 'BG_Pixel.png')}');
            }
        </style>
    </head>
    <body>
		<apex:form >
     		<apex:messages />
        	<apex:pageBlock >
             	<apex:pageBlockButtons >
                 	<apex:commandButton value="Create" action="{!save}"/>
        		 	<apex:commandButton value="Cancel" action="{!cancel}"/>
             	</apex:pageBlockButtons>
                <apex:pageBlockSection >
                	<apex:inputfield value="{!cmac.Barclays_SA__TitleText__c}" required="true" />
                </apex:pageBlockSection>
			</apex:pageBlock>
		</apex:form> 
	</body>
</apex:page>